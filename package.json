    "@folio/eslint-config-stripes": "^4.2.0",
    "@folio/stripes": "^3.0.0",
    "@folio/stripes-cli": "^1.14.0",
    "@folio/stripes-components": "^6.0.0",
    "@folio/stripes-connect": "^5.5.0",
    "@folio/stripes-core": "^4.0.0",
    "@folio/stripes-form": "^3.0.0",
    "@folio/stripes-final-form": "^2.0.0",
    "@folio/stripes-logger": "^1.0.0",
    "@folio/stripes-smart-components": "^3.0.0",
    "@folio/plugin-find-finc-metadata-collection": "^1.0.0",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^6.2.1",
    "mocha": "^5.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.1.1",
    "redux": "^4.0.0",
    "sinon": "^7.2.4"
  },
  "dependencies": {
    "final-form": "^4.18.7",
    "final-form-arrays": "^3.0.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react-final-form": "^6.3.5",
    "react-final-form-arrays": "^3.1.1",
    "react-dropzone": "^9.0.0",
    "react-intl": "^2.9.0",
    "react-router-dom": "^4.3.1",
    "react-router-prop-types": "^1.0.4",
    "redux-form": "^7.0.3"
  },
  "peerDependencies": {
    "@folio/stripes": "^3.0.0",
    "react": "*",
    "react-dom": "*",
    "react-intl": "*",
    "react-redux": "*",
    "react-router-dom": "*",
    "redux": "*"
  },
  "stripes": {
    "actsAs": ["app"],
    "displayName": "ui-finc-select.meta.title",
    "route": "/finc-select",
    "home": "/finc-select/metadata-sources",
    "queryResource": "query",
    "okapiInterfaces": {
      "finc-select/metadata-sources": "1.0",
      "finc-select/metadata-collections": "1.0",
      "finc-select/filters": "1.1",
      "finc-select/files": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.finc-select.enabled",
        "displayName": "UI: finc-select module is enabled",
        "visible": true
      },
      {
        "permissionName": "ui-finc-select.view",
        "displayName": "fincSelect: Can view metadatasources, metadatacollections and filters",
        "description": "Can view metadatasources, metadatacollections and filters",
        "subPermissions": [
          "module.finc-select.enabled",
          "fincSelect.view"
        ],
        "visible": true
      }
    ],
    "icons": [
      {
        "name": "app",
        "alt": "FINC select",
        "title": "fincSelect"
      }
    ]
  }
